{
    "name": "spryker/data-export",
    "type": "library",
    "description": "DataExport module",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "spryker/csv": "^3.0.0",
        "spryker/data-export-extension": "dev-new-data-export-process as 1.0.x-dev",
        "spryker/graceful-runner": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/log": "^3.0.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.27.0",
        "spryker/util-data-reader": "^1.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/",
            "Generated\\": "src/Generated/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vitaliiShveider/data-export-extension"
        }
    ]
}
